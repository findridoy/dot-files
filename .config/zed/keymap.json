[
    {
        "context": "Workspace",
        "bindings": {}
    },
    {
        "context": "Editor",
        "bindings": {
            "alt-i": "terminal_panel::ToggleFocus"
        }
    },
    {
        "context": "Terminal",
        "bindings": {
            "alt-i": "terminal_panel::ToggleFocus"
        }
    },
    {
        "context": "Editor && (vim_mode == insert)",
        "bindings": {
          "alt-h": "editor::MoveLeft",
          "alt-l": "editor::MoveRight"
        }
    }
]
