{
  "assistant": {
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-7-sonnet-latest"
    },
    "version": "2"
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 14.0,
  "theme": {
    "mode": "system",
    "light": "One Dark Pro",
    "dark": "One Dark"
  },
  "autosave": "on_focus_change",
  "languages": {
    "PHP": {
      "prettier": {
        "singleQuote": true
      }
    },
    "Blade": {
      "formatter": {
        "external": {
          "command": "blade-formatter",
          "arguments": ["{buffer_path}"]
        }
      }
    }
  },
  "file_types": {
    "Blade": ["*.blade.php"]
  }
}
